{
  "context": "SOCIAL",
  "contextSubType": "USER_GENERATED",
  "placementType": "IN_FEED",
  "assetTemplate": "awesomeAssets",
  "assetSets": [
    {
      "rule": "REQ_ALL",
      "assets": [
        {
          "key": "myTitle",
          "type": "TITLE",
          "title": {
            "maxLength": 75
          }
        }
      ]
    },
    {
      "rule": "REQ_ONE_PLUS",
      "assets": [
        {
          "key": "myVideo",
          "type": "VIDEO",
          "video": {
            "minDuration": 10,
            "maxDuration": 25,
            "protocols": [
              "VAST_3_0",
              "VAST_3_0_WRAPPER"
            ]
          }
        }
      ]
    },
    {
      "rule": "REQ_ONE_PLUS",
      "assets": [
        {
          "key": "myImage",
          "type": "IMAGE",
          "image": {
            "type": "MAIN",
            "width": 200,
            "widthMinimum": 150,
            "height": 300,
            "heightMinimum": 280
          }
        }
      ]
    },
    {
      "rule": "REQ_NONE",
      "assets": [
        {
          "key": "mySalesPriceData",
          "type": "DATA",
          "data": {
            "type": "SALE_PRICE",
            "maxLength": 120
          }
        }
      ]
    }
  ]
}
