<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>ssp-release</id>
      <configuration>
        <sshArgs>-o HostKeyAlgorithms=+ssh-dss</sshArgs>
        <scpArgs>-o HostKeyAlgorithms=+ssh-dss</scpArgs>
      </configuration>
    </server>
    <server>
      <id>ssp-snapshots</id>
      <configuration>
        <sshArgs>-o HostKeyAlgorithms=+ssh-dss</sshArgs>
        <scpArgs>-o HostKeyAlgorithms=+ssh-dss</scpArgs>
      </configuration>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>onemobile-properties</id>
      <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.javadoc.failOnError>false</maven.javadoc.failOnError>
      </properties>
    </profile>
    <!-- OneMobile repositories only (do not edit) -->
    <profile>
      <id>onemobile-repositories</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <!--
          This is a virtual repository that includes:
          - onemobile-release
          - maven-internal-nexage-releases (Nexage Artifactory)
          - jcentral
        -->
        <repository>
          <id>onemobile-central</id>
          <url>https://artifactory.ouroath.com/artifactory/onemobile-maven-release</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </releases>
        </repository>
        <repository>
          <id>onemobile-snapshot</id>
          <url>https://artifactory.ouroath.com/artifactory/onemobile-snapshot</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>onemobile-plugin-release</id>
          <url>https://artifactory.ouroath.com/artifactory/onemobile-plugin-release</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </releases>
        </pluginRepository>
        <pluginRepository>
          <id>onemobile-plugin-snapshot</id>
          <url>https://artifactory.ouroath.com/artifactory/onemobile-plugin-snapshot</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </pluginRepository>

      </pluginRepositories>
    </profile>
    <!-- Oath repositories - add here third party repos -->
    <profile>
      <id>oath-artifactory</id>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>ssp-release</id>
          <url>https://artifactory.ouroath.com/artifactory/ssp-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>aol-advertising-local</id>
          <url>https://artifactory.ouroath.com/artifactory/aol-advertising-local</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-maven-local-release</id>
          <url>https://artifactory.ouroath.com/artifactory/maven-local-release</url>
        </repository>
        <repository>
          <snapshots/>
          <id>oath-maven-local-snapshot</id>
          <url>https://artifactory.ouroath.com/artifactory/maven-local-snapshot</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-adv-ds-maven-local-release</id>
          <url>https://artifactory.ouroath.com/artifactory/adv-ds-maven-local-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-guce-releases</id>
          <url>https://artifactory.ouroath.com/artifactory/guce-releases</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-libs-release</id>
          <url>https://artifactory.ouroath.com/artifactory/libs-release</url>
        </repository>
        <repository>
          <snapshots/>
          <id>oath-libs-snapshot</id>
          <url>https://artifactory.ouroath.com/artifactory/libs-snapshot</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-crs-central</id>
          <url>https://artifactory.ouroath.com/artifactory/crs-central</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-maven-dmp-us-east-1-local</id>
          <url>https://artifactory.ouroath.com/artifactory/maven-dmp-us-east-1-local</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>profile-services</id>
          <url>https://artifactory.ouroath.com/artifactory/profile-services</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>targeting-dublin-maven-local</id>
          <url>https://artifactory.ouroath.com/artifactory/targeting-dublin-maven-local</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>tnv-maven-local-release</id>
          <url>https://artifactory.ouroath.com/artifactory/tnv-maven-local-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>java-platform-maven-release</id>
          <url>https://artifactory.ouroath.com/artifactory/java-platform-maven-release</url>
        </repository>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>maven-release</id>
          <url>https://artifactory.ouroath.com/artifactory/maven-release</url>
        </repository>
        <repository>
          <id>jcenter-cache</id>
          <url>https://artifactory.ouroath.com/artifactory/jcenter-cache</url>
        </repository>
        <repository>
          <id>maven-org-cache</id>
          <url>https://artifactory.ouroath.com/artifactory/maven-org-cache</url>
        </repository>
        <repository>
          <id>oath-artifactory</id>
          <name>oath-artifactory</name>
          <url>https://edge.artifactory.yahoo.com:4443/artifactory/maven-release</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>artifactory-maven-remote-repos</id>
          <name>artifactory-maven-remote-repos</name>
          <url>https://edge.artifactory.yahoo.com:4443/artifactory/maven-remote-repos</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <layout>default</layout>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>oath-plugins-release</id>
          <url>https://artifactory.ouroath.com/artifactory/plugins-release</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots/>
          <id>oath-plugins-snapshot</id>
          <url>https://artifactory.ouroath.com/artifactory/plugins-snapshot</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>onemobile-properties</activeProfile>
    <activeProfile>onemobile-repositories</activeProfile>
    <activeProfile>oath-artifactory</activeProfile>
  </activeProfiles>
</settings>
